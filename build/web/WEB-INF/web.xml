<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/j2ee/dtds/web-app_2_3.dtd">
<web-app>

    <servlet>
        <servlet-name>JasapController</servlet-name>
        <servlet-class>br.jasap.core.JasapController</servlet-class>
        <init-param>
            <param-name>APPLICATION_MANAGER</param-name>
            <param-value>app.RootManager</param-value>
        </init-param>
        <load-on-startup>0</load-on-startup>
    </servlet>

    <servlet-mapping>
        <servlet-name>JasapController</servlet-name>
        <url-pattern>*.jsap</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
    
</web-app>